{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${DATABASE_URL}",
      "Redis": "${REDIS_URL}"
  },
  "Jwt": {
    "Key": "${JWT_KEY}",
    "Issuer": "ControlGastosAPI",
    "Audience": "ControlGastosClients",
    "ExpirationMinutes": "60"
  }
}
